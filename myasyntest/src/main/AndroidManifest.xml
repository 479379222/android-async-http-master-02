<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myas.myasyntest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/accessibility_64"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyListActivity" />
        <activity android:name=".ArraylistActivity" />
        <activity android:name=".SimplelistActivity" />
        <activity android:name=".SimplecursorActivity" />
        <activity android:name=".ReadcontactActivity" />
        <activity android:name=".MyserviceActivity" />
        <activity android:name=".BaseserviceActivity" />
        <activity android:name=".http.HttpGetActivity" />
        <activity android:name=".http.HttpmainActivity" />
        <activity android:name=".http.HttppostActivity" />
        <activity android:name=".http.UploadingActivity" />
        <activity android:name=".http.JsonActivity"></activity>
    </application>

</manifest>
